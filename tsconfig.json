{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/general/shop.tsx",
    "app/[lang]/page.tsx",
    "components/saved/saved.tsx",
    "components/product_card/prices/prices-options.tsx",
    "app/[lang]/shop/quick_mix/page.tsx",
    "app/[lang]/saved/page.tsx",
    "app/[lang]/cart/page.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}